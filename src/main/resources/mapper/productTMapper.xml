<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.pharmwebspring.DAO.ProductDaoT">
    <select id = "listProductT" resultType = "com.example.pharmwebspring.Model.ProductT">
            select * from parmdb.product_t;
        </select>
    <select id="detailProductT" resultType="com.example.pharmwebspring.Model.ProductT">
                select * from product_t where prodT_name=#{prodT_name}
        </select>
</mapper>