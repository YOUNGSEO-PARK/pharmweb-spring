<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.pharmwebspring.DAO.ProductDaoA">
    <select id = "listProductA" resultType = "com.example.pharmwebspring.Model.ProductA">
            select * from parmdb.product_a;
        </select>
    <select id="detailProductA" resultType="com.example.pharmwebspring.Model.ProductA">
                select * from product_a where prodA_name=#{prodA_name}
        </select>
</mapper>